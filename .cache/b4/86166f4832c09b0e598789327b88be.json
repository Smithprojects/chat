crollbar-width: none;//скрыть полосу прокрутки ff\r\n          \r\n            &::-webkit-scrollbar {//скрыть полосу прокрутки chrome\r\n                width:0px;\r\n            }\r\n        }\r\n\r\n        &__card-a {\r\n\r\n        }\r\n\r\n        .card-a {\r\n            display: flex;\r\n            flex-direction: column;\r\n            align-items: flex-end;\r\n            // justify-content: space-around;\r\n            // align-items: center;\r\n            // height: 16%;\r\n            min-height: 16%;\r\n            margin-top: 5px;\r\n            // position: relative;\r\n            // top: -40px;\r\n\r\n            &_f-start {\r\n                align-items: flex-start;\r\n            }\r\n\r\n            &__text {\r\n                margin: 0 5px;\r\n                // background-size: cover;\r\n                // width: 100%;\r\n                height: 100%;\r\n                // width: 75%;\r\n                width: auto;\r\n                min-width: 50px;\r\n\r\n                &_form-1 {\r\n                    background: url('/image/chat-form-1.png') no-repeat;\r\n                    background-size: 88% 100%;\r\n                }\r\n                \r\n                &_form-2 {\r\n                    background: url('/image/chat-form-2.png') no-repeat;\r\n                    background-size: 88% 100%;\r\n                }\r\n            }\r\n\r\n            &__text-position {\r\n                width: 86%;\r\n                height: 74%;\r\n                // border: 1px solid yellow;\r\n                // padding-left: 20px;\r\n                font-size: 10px;\r\n                // padding-top: 12px;\r\n                padding: 10px;\r\n            }\r\n\r\n            &__autor {\r\n                // position: relative;\r\n                // top: 216%;\r\n            }\r\n\r\n            &__avatar {\r\n                width: 40px;\r\n                height: 40px;\r\n                border: 1px solid black;\r\n                border-radius: 50%;\r\n                margin: 0 auto;\r\n                \r\n            }\r\n\r\n            &__img {\r\n                width: 100%;\r\n                height: 100%;\r\n                border-radius: 50%;\r\n                box-shadow: 2px 3px 10px rgb(0,0,0,1);\r\n            }\r\n\r\n            &__name {\r\n\r\n            }\r\n\r\n            &__name-text {\r\n                text-align: center;\r\n                font-size: 10px;\r\n                margin: 0;\r\n                \r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n.btn-svg {\r\n    width: 40px;\r\n}\r\n\r\n@keyframes neon {\r\n    from {\r\n      text-shadow:\r\n      0 0 6px rgba(202,228,225,0.92),\r\n      0 0 30px rgba(202,228,225,0.34),\r\n      0 0 12px rgba(30,132,242,0.52),\r\n      0 0 21px rgba(30,132,242,0.92),\r\n      0 0 34px rgba(30,132,242,0.78),\r\n      0 0 54px rgba(30,132,242,0.92);\r\n    }\r\n    to {\r\n      text-shadow:\r\n      0 0 6px rgba(202,228,225,0.98),\r\n      0 0 30px rgba(202,228,225,0.42),\r\n      0 0 12px rgba(30,132,242,0.58),\r\n      0 0 22px rgba(30,132,242,0.84),\r\n      0 0 38px rgba(30,132,242,0.88),\r\n      0 0 60px rgba(30,132,242,1);\r\n    }\r\n  }\r\n  \r\n  \r\n  \r\n  // Link\r\n  .link {\r\n    position: absolute;\r\n    bottom: 10px; left: 10px;\r\n    color: #828282;\r\n    text-decoration: none;\r\n    \r\n    &:focus,\r\n    &:hover {\r\n      color: #c6e2ff;\r\n      text-shadow:\r\n      0 0 2px rgba(202,228,225,0.92),\r\n      0 0 10px rgba(202,228,225,0.34),\r\n      0 0 4px rgba(30,132,242,0.52),\r\n      0 0 7px rgba(30,132,242,0.92),\r\n      0 0 11px rgba(30,132,242,0.78),\r\n      0 0 16px rgba(30,132,242,0.92);\r\n    }\r\n  }"},"lineCount":null}},"error":null,"hash":"322c19dd3a305700105659e0663b3102","cacheData":{"env":{}}}